{
  "manifest_version": 3,
  "name": "Zing-Blocker",
  "version": "1.0",
  "description": "Blocks specified websites",
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/image.png"
  }
}
